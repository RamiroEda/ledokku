{
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll": true,
        "source.organizeImports": true,
        "source.sortMembers": true
    },
    "files.eol": "\n",
    "jest.jestCommandLine": "jest",
    "jest.autoRun": {
        "onStartup": []
    },
    "nuxt.isNuxtApp": false,
    "debug.inlineValues": true
}