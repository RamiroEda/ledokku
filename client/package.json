{
  "name": "ledokku-client",
  "private": true,
  "version": "0.7.0",
  "license": "MIT",
  "scripts": {
    "dev": "yarn watch:css && react-scripts start",
    "build": "yarn build:css && react-scripts build",
    "test": "react-scripts test",
    "build:css": "postcss src/styles/index.css -o src/generated/index.css",
    "watch:css": "postcss src/styles/index.css -o src/generated/index.css",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "proxy": "http://localhost:4000",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "react/react-in-jsx-scope": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.7.0",
    "@chakra-ui/react": "1.5.0",
    "@emotion/react": "11.4.0",
    "@emotion/styled": "11.3.0",
    "@headlessui/react": "0.2.0",
    "@nextui-org/react": "^1.0.0-beta.10",
    "ansi_up": "5.0.0",
    "classnames": "2.2.6",
    "date-fns": "^2.29.3",
    "fathom-client": "3.0.0",
    "focus-visible": "5.2.0",
    "formik": "2.2.6",
    "framer-motion": "4.0.0",
    "graphql": "15.3.0",
    "graphql-ws": "^5.11.2",
    "jwt-decode": "3.1.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-icons": "^4.4.0",
    "react-innertext": "^1.1.5",
    "react-router-dom": "5.2.0",
    "react-select": "4.3.0",
    "react-syntax-highlighter": "^15.5.0",
    "react-terminal-ui": "^1.0.0",
    "react-toastify": "9.0.0",
    "styled-components": "5.2.1",
    "subscriptions-transport-ws": "0.9.18",
    "use-dark-mode": "^2.3.1",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^3.2.1",
    "@graphql-codegen/cli": "^2.13.1",
    "@graphql-codegen/typescript": "^2.7.3",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@graphql-codegen/typescript-react-apollo": "^3.3.3",
    "@tailwindcss/forms": "^0.5.3",
    "@types/classnames": "2.2.11",
    "@types/node": "14.14.2",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-router-dom": "5.1.6",
    "@types/react-select": "4.0.14",
    "@types/react-syntax-highlighter": "^15.5.5",
    "@types/react-transition-group": "4.4.0",
    "@types/styled-components": "5.1.7",
    "autoprefixer": "10.0.2",
    "postcss": "8.1.9",
    "postcss-cli": "8.3.0",
    "react-scripts": "4.0.3",
    "tailwindcss": "^3.1.8",
    "typescript": "4.2.2"
  }
}
