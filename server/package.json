{
  "name": "ledokku-server",
  "private": true,
  "version": "0.7.0",
  "license": "MIT",
  "scripts": {
    "build": "yarn generate && yarn tsc",
    "barrels": "yarn barrels:modules && yarn barrels:repos",
    "barrels:modules": "barrelsby --config .barrelsby.json",
    "barrels:repos": "barrelsby --config .barrelsby.repositories.json",
    "start": "yarn prisma:migrate && node build/index.js",
    "dev": "yarn prisma:migrate && yarn generate && nodemon 'src/index.ts' --exec \"npx ts-node src/index.ts\" -e ts,graphql",
    "generate": "yarn run barrels && yarn prisma:generate",
    "tsc": "tsc --project tsconfig.json",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:migrate:save": "prisma migrate dev"
  },
  "dependencies": {
    "@octokit/auth-app": "^4.0.7",
    "@octokit/oauth-app": "^4.2.0",
    "@octokit/rest": "^19.0.5",
    "@octokit/types": "^8.0.0",
    "@prisma/client": "^4.7.1",
    "@tsed/common": "^7.8.2",
    "@tsed/core": "^7.8.2",
    "@tsed/di": "^7.8.2",
    "@tsed/exceptions": "^7.8.2",
    "@tsed/json-mapper": "^7.8.2",
    "@tsed/platform-express": "^7.8.2",
    "@tsed/schema": "^7.8.2",
    "@tsed/typegraphql": "^7.8.2",
    "apollo-datasource": "^3.3.2",
    "apollo-datasource-rest": "^3.7.0",
    "apollo-server": "^3.10.2",
    "apollo-server-core": "^3.10.2",
    "apollo-server-express": "^3.10.2",
    "body-parser": "^1.20.0",
    "bullmq": "^2.1.3",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "execa": "^6.1.0",
    "express": "4.17.1",
    "graphql": "15.3.0",
    "graphql-scalars": "^1.18.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.11.2",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "node-fetch": "2.6.6",
    "node-ssh": "^13.0.0",
    "prisma": "^4.7.1",
    "sshpk": "^1.17.0",
    "tslib": "^2.4.0",
    "type-graphql": "^1.1.1",
    "ws": "^8.9.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/jsonwebtoken": "8.5.0",
    "@types/node": "14.14.2",
    "@types/node-ssh": "7.0.0",
    "@types/sshpk": "^1.17.0",
    "apollo-server-testing": "^2.25.3",
    "barrelsby": "^2.5.1",
    "nodemon": "^2.0.20",
    "smee-client-patched": "1.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
