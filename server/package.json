{
  "name": "ledokku-server",
  "private": true,
  "version": "0.7.0",
  "license": "MIT",
  "scripts": {
    "dev": "yarn prisma:migrate && yarn prisma:generate && nodemon 'src/index.ts' --exec \"npx ts-node src/index.ts\" -e ts,graphql",
    "build": "tsc",
    "start": "yarn prisma:migrate && node build/index.js",
    "generate": "graphql-codegen --config codegen.yml",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy --preview-feature",
    "prisma:migrate:save": "prisma migrate dev --preview-feature"
  },
  "dependencies": {
    "@graphql-tools/schema": "^9.0.4",
    "@octokit/auth-app": "3.3.0",
