    "@octokit/types": "6.14.1",
    "@prisma/client": "2.17.0",
    "@tsed/common": "^6.133.0",
    "@tsed/core": "^6.133.0",
    "@tsed/di": "^6.133.0",
    "@tsed/exceptions": "^6.133.0",
    "@tsed/json-mapper": "^6.133.0",
    "@tsed/platform-express": "^6.133.0",
    "@tsed/schema": "^6.133.0",
    "@tsed/typegraphql": "^6.133.0",
    "apollo-datasource": "^3.3.2",
    "apollo-datasource-rest": "^3.7.0",
    "apollo-server": "^3.10.2",
    "apollo-server-core": "^3.10.2",
    "apollo-server-express": "2.21.0",
    "body-parser": "^1.20.0",
    "bullmq": "1.9.0",
    "compression": "^1.7.4",
