    "graphql-subscriptions": "^2.0.0",
    "jsonwebtoken": "8.5.1",
    "method-override": "^3.0.0",
    "node-fetch": "2.6.1",
    "node-ssh": "11.1.1",
    "prisma": "2.17.0",
